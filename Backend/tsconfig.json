   "compilerOptions": {
      "lib": ["DOM", "ESNext", "ES2023"],
      "target": "ES2023",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "esModuleInterop": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true
